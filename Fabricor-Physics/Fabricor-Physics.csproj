<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>Fabricor.Physics</RootNamespace>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <PackageId>Fabricor.Physics</PackageId>
    <Version>1.0.0</Version>
    <Authors>SamHSmith</Authors>
    <Title>A Physics Engine based on the Fabricor.ECS library</Title>
    <PackageDescription>Fabricor.Physics is a physics engine made using the Fabricor.ECS library.</PackageDescription>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/SamHSmith/Fabricor-Physics</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Fabricor.ECS" Version="1.0.0" />
  </ItemGroup>

</Project>
